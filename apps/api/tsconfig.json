{
	"compilerOptions": {
		"target": "ES2021",
		"module": "ES2022",
		"moduleResolution": "Bundler",
		"types": ["bun-types"],
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"baseUrl": "./src",
		"paths": {
			"@/*": ["*"]
		}
	}
}
